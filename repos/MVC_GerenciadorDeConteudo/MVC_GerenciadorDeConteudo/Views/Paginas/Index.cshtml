@{
    ViewBag.Title = "Paginas";
    List<Business.Pagina> paginas = ViewBag.Paginas; //os dados são armazenados na var paginas
}

<div class="jumbotron">
    <h1>Paginas</h1>
    <div>
        <a href="/paginas/novo" class="btn btn-default">Cadastrar   </a>
    </div>
    <table class="table">

        <tr>
            <td>ID</td>
            <td>Nome</td>
            <td>Data</td>
        </tr>
        @foreach (var p in paginas)
        {
            <tr>
                <td>@p.Id</td>
                <td>@p.Nome</td>
                <td>@p.Time</td>
            </tr>
        }

    </table>
</div>